    "picocolors": "^1.1.1",
    "sigstore": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
