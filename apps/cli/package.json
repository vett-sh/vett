{
  "name": "vett",
  "version": "0.1.3",
  "description": "CLI for the Vett secure agent skill registry",
  "type": "module",
  "bin": {
    "vett": "./dist/index.mjs"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vett-sh/vett.git",
    "directory": "apps/cli"
  },
  "homepage": "https://vett.sh",
