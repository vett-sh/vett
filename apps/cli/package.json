{
  "name": "vett",
  "version": "0.1.0",
  "description": "CLI for the Vett secure agent skill registry",
  "type": "module",
  "bin": {
    "vett": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "repository": {
