    "@types/node": "^22.10.0",
    "@types/semver": "^7.7.1",
    "@vett/core": "workspace:*",
    "zod": "^3.23.8",
    "commander": "^12.1.0",
    "picocolors": "^1.1.1",
    "semver": "^7.7.3",
    "sigstore": "^4.1.0",
    "tsdown": "^0.20.3",
    "typescript": "^5.6.3",
    "vitest": "^3.2.4"
